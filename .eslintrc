{
  "extends": ["eslint:recommended", "plugin:prettier/recommended", "plugin:mocha/recommended"],
  "plugins": ["prettier", "mocha"],
  "rules": {
    "prettier/prettier": ["error"]
  },
  "parserOptions": {
    "ecmaVersion": 2019
  },
  "env": {
    "commonjs": true,
    "es6": true,
    "node": true,
    "mocha": true
  }
}
